{#
  Custom template for the 'field_director' field on node.
#}
<div{{ attributes.addClass('field__director') }}>
  {% if label %}
    <div class="field__label">{{ label }}</div>
  {% endif %}
  <div class="field__items">
    {% for item in items %}
      <div class="field__item">{{ item.content }}</div>
    {% endfor %}
  </div>
</div>

<hr class="field-separator">